{
  "FLog": {
    "Initialization": {
      "AsyncBufferingInit": {
        "AsyncProcessingType": "ConfigDefinedAsyncThreads",
        "InitialAsyncProcessing" : 2
      }
    },
    "Appenders": {
      "ColoredConsole": {
        "AppenderType": "ConsoleOut",
        "LogEntryFormatLayout": "'%FC,DarkCyan%''%TS:yyyy-MM-dd%' '%FC,DarkGreen%''%TS:HH:mm:ss%''%FC,DarkCyan%''%TS:.fff%''%FC,Cyan%''%TS_US%' '%FCM,Red,DarkYellow,White,Gray,DarkGray%''%LVL,-5%' '%FC,DarkGray%'['%THREADNAME,10[..15]%' '%THREADID%'] '%FC,Blue%''%LGR,[^2..]%':'%FC,Cyan%''%LLN,3:00%' '%FCM,Red,DarkYellow,White,Gray,DarkGray%''%MSG%''%RC%''%NL%'",
        "CharBufferSize": 8000,
        "EnableDoubleBufferToggling" : true,
        "FlushConfig": {
          "WriteTriggeredAfterTimeSpan": {
            "Seconds": 2
          },
          "AutoTriggeredAfterTimeSpan": {
            "Seconds": 2
          }
        },
        "RunOnAsyncQueueNumber": 1,
        "FlushAsyncQueueNumber": 2
      }
    },
    "RootLogger": {
      "Appenders" : {
        "ColoredConsole" : {}
      }
    }
  }
}