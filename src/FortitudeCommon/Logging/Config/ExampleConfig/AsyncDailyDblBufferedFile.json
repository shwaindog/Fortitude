{
  "FLog": {
    "Initialization": {
      "AsyncBufferingInit": {
        "AsyncProcessingType": "ConfigDefinedAsyncThreads",
        "InitialAsyncProcessing" : 2
      }
    },
    "Appenders": {
      "AppLogFileAppender": {
        "AppenderType": "FileUnbounded",
        "LogEntryFormatLayout": "'%TS:yyyy-MM-dd HH:mm:SS.fff%' '%LVL,5%' ['%THREADNAME,10[..15]%' '%THREADID%'] '%LGR%':'%LLN:-3%' '%MSG%''%NL%'",
        "CharBufferSize": 8000,
        "EnableDoubleBufferToggling" : true,
        "FlushConfig": {
          "WriteTriggeredAfterTimeSpan": {
            "Seconds": 2
          }
        },
        "RunOnAsyncQueueNumber": 1,
        "FlushAsyncQueueNumber": 2,
        "FilePath": "Logs/",
        "FileName": "'%StartAssemblyName%'-'%TS:MMdd%'.log"
      }
    },
    "RootLogger": {
      "Appenders" : {
        "AppLogFileAppender" : {}
      }
    }
  }
}