{
  "FLog": {
    "Appenders": {
      "ForwardingAppender": {
        "AppenderType": "ForwardingAppender",
        "ForwardToAppenders": {
          "AppLogFileAppender": {
            "AppenderType": "FileUnbounded",
            "LogEntryFormatLayout": "'%TS:yyyy-MM-dd HH:mm:ss.ffffff%' '%LVL,5%' ['%THREADNAME,10[..15]%' '%THREADID%'] '%LGR%':'%LLN,3:00%' '%MSG%''%NL%'",
            "DisableBuffering": true,
            "FilePath": "Logs/",
            "FileName": "'%StartAssemblyName%'-'%TS:MMdd%'.log"
          },
          "ColoredConsole" : {
            "AppenderType": "ConsoleOut",
            "LogEntryFormatLayout": "'%FCM,Red,DarkYellow,White,Gray,DarkGray:'%TS:yyyy-MM-dd HH:mm:ss.ffffff%' '%LVL,5%' ['%THREADNAME,10[..15]%' '%THREADID%'] '%LGR%':'%LLN,3:00%' '%MSG%''%NL%' %'",
            "DisableBuffering": true
          }
        }
      }
    },
    "RootLogger": {
      "Appenders" : {
        "ForwardingAppender" : {}
      }
    }
  }
}