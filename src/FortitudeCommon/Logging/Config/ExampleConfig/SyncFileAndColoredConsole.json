{
  "FLog": {
    "Appenders": {
      "ForwardingAppender": {
        "AppenderType": "ForwardingAppender",
        "ForwardToAppenders": {
          "AppLogFileAppender": {
            "AppenderType": "FileUnbounded",
            "LogEntryFormatLayout": "'%TS:yyyy-MM-dd HH:mm:ss.ffffff%' '%LVL,5%' ['%THREADNAME,10[..15]%' '%THREADID%'] '%LGR%':'%LLN,3:00%' '%MSG%''%NL%'",
            "DisableBuffering": true,
            "FilePath": "Logs/",
            "FileName": "'%StartAssemblyName%'-'%TS:MMdd%'.log"
          },
          "ColoredConsole" : {
            "AppenderType": "ConsoleOut",
            "LogEntryFormatLayout": "'%FC,DarkCyan:'%TS:yyyy-MM-dd HH:%'%''%FC,Cyan:'%TS:mm:ss.ffffff%'%' '%FCM,Red,DarkYellow,White,Gray,DarkGray:'%LVL,5%'%' '%FC,DarkGray:['%THREADNAME,10[..15]%' '%THREADID%']%' '%FC,DarkBlue:'%LGR%':%''%FC,Blue:'%LLN,3:00%'%' '%FCM,Red,DarkYellow,White,Gray,DarkGray:'%MSG%'%'%NL%'",
            "DisableBuffering": true
          }
        }
      }
    },
    "RootLogger": {
      "Appenders" : {
        "ForwardingAppender" : {}
      }
    }
  }
}